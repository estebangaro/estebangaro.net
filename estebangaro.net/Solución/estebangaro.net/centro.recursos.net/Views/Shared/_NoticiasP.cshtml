@model List<centro.recursos.net.Models.Entity_Framework.NoticiaPrincipal>
@using centro.recursos.net.Models.Helpers

<div class="noticiasR">
    <p class="tituloNR">Publicaciones recientes</p>
    @Html.GeneraNoticias(Model.Take(4).ToList())
</div>
<div class="noticiasR">
    <p class="tituloNR">Publicaciones mas comentadas</p>
    @Html.GeneraNoticias(Model.Skip(4).Take(4).ToList())
</div>